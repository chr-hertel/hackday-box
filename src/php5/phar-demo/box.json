{
  "alias": "demo.phar",
  "output": "demo.phar",
  "chmod": "0755",
  "compactors": [
    "Herrera\\Box\\Compactor\\Php"
  ],
  "main": "bin/cli",
  "finder": [
    {
    "name": "*.php",
    "in": "src"
    },
    {
      "name": "*.*",
      "exclude": ["Tests"],
      "in": "vendor"
    }
  ],
  "stub": true
}